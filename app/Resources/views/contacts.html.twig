{% extends 'layouts/master.html.twig' %}
{% block title %} Contacts {% endblock %}
{% block styles %}
    <link href="{{ asset('css/contacts.css') }}" rel="stylesheet">
{% endblock %}
{% block header %} {% include 'parts/header.html.twig' %} {% endblock %}
{% block content %}
    <div class="col-lg-1"></div>
    <div class="col-lg-10 mt-3 mb-3">
        <div class="col-lg-4">
            <h6 class="text-uppercase font-weight-bold">
                <strong>Contact</strong>
            </h6>
            <hr class="deep-purple accent-2 mb-4 mt-0 d-inline-block mx-auto" style="width: 60px;">
            <p>
                <i class="fa fa-home mr-3"></i> New York, NY 10012, US</p>
            <p>
                <i class="fa fa-envelope mr-3"></i> info@example.com</p>
            <p>
                <i class="fa fa-phone mr-3"></i> + 01 234 567 88</p>
            <p>
                <i class="fa fa-print mr-3"></i> + 01 234 567 89</p>
        </div>
        <div class="col-lg-6">
            <iframe src="https://www.google.com/maps/embed?pb=!1m16!1m12!1m3!1d2965.0824050173574!2d-93.63905729999999!3d41.998507000000004!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!2m1!1sWebFilings%2C+University+Boulevard%2C+Ames%2C+IA!5e0!3m2!1sen!2sus!4v1390839289319"
                    width="100%" height="200" frameborder="0" style="border:0"></iframe>
        </div>
    </div>
    <div class="col-lg-1"></div>
{% endblock %}
{% block footer %} {% include 'parts/footer.html.twig' %} {% endblock %}